{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "dragonmountsplus:cherry_particle",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particle/dm_cherry_particle"
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 64,
				"max_particles": 128
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 4.4
			},
			"minecraft:emitter_shape_point": {
				"offset": ["Math.random(-1.4, 1.4)", "Math.random(-0.6, 0.0)", "Math.random(-1.4, 1.4)"]
			},
			"minecraft:particle_initialization": {
				"per_update_expression": "variable.stuck_time = 2.0;",
				"per_render_expression": "variable.stuck_time = 2.0;"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "Math.random(1.6, 4.4)"
			},
			"minecraft:particle_initial_speed": 0,
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, -0.8, 0],
				"linear_drag_coefficient": "variable.stuck_time = 2.0;return variable.particle_age < variable.stuck_time ? 277 : 0.01;"
			},
			"minecraft:particle_appearance_billboard": {
				"size": [0.15, 0.15],
				"facing_camera_mode": "lookat_xyz",
				"uv": {
					"texture_width": 16,
					"texture_height": 16,
					"uv": ["Math.floor(v.particle_random_3 * 2) * 8", "Math.floor(v.particle_random_4 * 2) * 8"],
					"uv_size": [8, 8]
				}
			},
			"minecraft:particle_motion_collision": {
				"collision_drag": 5,
				"coefficient_of_restitution": 0.48,
				"collision_radius": 0.1
			},
			"minecraft:particle_appearance_lighting": {},
			"minecraft:particle_appearance_tinting": {
				"color": [1, 1, 1, 1]
			}
		}
	}
}
